@inproceedings{pessoa_generic_2019,
	location = {Cham},
	title = {A Generic Exact Solver for Vehicle Routing and Related Problems},
	isbn = {978-3-030-17953-3},
	doi = {10.1007/978-3-030-17953-3_27},
	series = {Lecture Notes in Computer Science},
	abstract = {Major advances were recently obtained in the exact solution of Vehicle Routing Problems ({VRPs}). Sophisticated Branch-Cut-and-Price ({BCP}) algorithms for some of the most classical {VRP} variants now solve many instances with up to a few hundreds of customers. However, adapting and reimplementing those successful algorithms for other variants can be a very demanding task. This work proposes a {BCP} solver for a generic model that encompasses a wide class of {VRPs}. It incorporates the key elements found in the best recent {VRP} algorithms: ng-path relaxation, rank-1 cuts with limited memory, and route enumeration; all generalized through the new concept of “packing set”. This concept is also used to derive a new branch rule based on accumulated resource consumption and to generalize the Ryan and Foster branch rule. Extensive experiments on several variants show that the generic solver has an excellent overall performance, in many problems being better than the best existing specific algorithms. Even some non-{VRPs}, like bin packing, vector packing and generalized assignment, can be modeled and effectively solved.},
	pages = {354--369},
	booktitle = {Integer Programming and Combinatorial Optimization},
	publisher = {Springer International Publishing},
	author = {Pessoa, Artur and Sadykov, Ruslan and Uchoa, Eduardo and Vanderbeck, François},
	editor = {Lodi, Andrea and Nagarajan, Viswanath},
	date = {2019},
	langid = {english},
	keywords = {Column generation, Integer programming, Routing},
	file = {Full Text PDF:C\:\\Users\\Lisa Salewsky\\OneDrive\\Dokumente\\Uni\\Dortmund\\Master\\MA\\LS11 (Buchin)\\Literatur\\Ls11 Buchin MA Literatur\\files\\872\\Pessoa et al. - 2019 - A Generic Exact Solver for Vehicle Routing and Rel.pdf:application/pdf},
}

@inproceedings{baskaya_generic_2009,
	title = {A generic reconfigurable array specification and programming environment ({GRASPER})},
	url = {https://ieeexplore.ieee.org/abstract/document/5275071?casa_token=NGH9B30tNVcAAAAA:f-Qg14uJDAGFDmaHZKw1ApkU5sqPji5JiHWevc9mgqHXT0UvxEmGk3EVjww8nnSo21brHf8u},
	doi = {10.1109/ECCTD.2009.5275071},
	abstract = {Modern advances in reconfigurable technologies are allowing analog circuit designers to benefit from the computational flexibility provided by large-scale field-programmable analog arrays ({FPAAs}). With the component density of these devices, small analog circuits as well as larger analog systems can be synthesized and tested in a shorter time and at a lower cost compared to the full design cycle. However, automated development platforms and {CAD} tools for these devices are far fewer than the physical synthesis tools for their digital counterparts. One of the major reasons for this is the considerably higher impact of interconnect parasitics on circuit functionality in the analog domain; therefore, performance metrics must be monitored closely. Our goal in this paper is to present a physical synthesis framework with a generic architecture specification interface and a parasitic extractor for verification of the synthesis results. Our synthesis tool can support a wide range of {FPAA} architectures and our simulations can successfully predict the performance metrics.},
	eventtitle = {2009 European Conference on Circuit Theory and Design},
	pages = {619--622},
	booktitle = {2009 European Conference on Circuit Theory and Design},
	author = {Baskaya, Faik and Anderson, David V. and Hasler, Paul and Lim, Sung Kyu},
	urldate = {2023-10-22},
