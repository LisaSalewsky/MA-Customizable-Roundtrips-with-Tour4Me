	file = {IEEE Xplore Abstract Record:C\:\\Users\\Lisa Salewsky\\OneDrive\\Dokumente\\Uni\\Dortmund\\Master\\MA\\LS11 (Buchin)\\Literatur\\Ls11 Buchin MA Literatur\\files\\879\\5275071.html:text/html;IEEE Xplore Full Text PDF:C\:\\Users\\Lisa Salewsky\\OneDrive\\Dokumente\\Uni\\Dortmund\\Master\\MA\\LS11 (Buchin)\\Literatur\\Ls11 Buchin MA Literatur\\files\\874\\Baskaya et al. - 2009 - A generic reconfigurable array specification and p.pdf:application/pdf},
