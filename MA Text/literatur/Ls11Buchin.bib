@article{stroobant_generating_2018,
	title = {Generating constrained length personalized bicycle tours},
	volume = {16},
	issn = {1614-2411},
	url = {https://doi.org/10.1007/s10288-018-0371-9},
	doi = {10.1007/s10288-018-0371-9},
	abstract = {In the context of recreational routing, the problem of finding a route which starts and ends in the same location (while achieving a length between specified upper and lower boundaries) is a common task, especially for tourists or cyclists who want to exercise. The topic of finding a tour between a specified starting and ending location while minimizing one or multiple criteria is well covered in literature. In contrast to this, the route planning task in which a pleasant tour with length between a maximum and a minimum boundary needs to be found is relatively underexplored. In this paper, we provide a formal definition of this problem, taking into account the existing literature on which route attributes influence cyclists in their route choice. We show that the resulting problem is {NP}-hard and devise a branch-and-bound algorithm that is able to provide a bound on the quality of the best solution in pseudo-polynomial time. Furthermore, we also create an efficient heuristic to tackle the problem and we compare the quality of the solutions that are generated by the heuristic with the bounds provided by the branch-and-bound algorithm. Also, we thoroughly discuss the complexity and running time of the heuristic.},
	pages = {411--439},
	number = {4},
	journaltitle = {4OR},
	shortjournal = {4OR-Q J Oper Res},
	author = {Stroobant, P. and Audenaert, P. and Colle, D. and Pickavet, M.},
	urldate = {2023-10-22},
	date = {2018-12-01},
	langid = {english},
	note = {Number: 4},
	keywords = {94C15, Branch-and-bound algorithm, Constrained bicycle routing, Primary 05C38, Reach-based routing, Secondary 68R10},
	file = {Full Text PDF:C\:\\Users\\Lisa Salewsky\\OneDrive\\Dokumente\\Uni\\Dortmund\\Master\\MA\\LS11 (Buchin)\\Literatur\\Ls11 Buchin MA Literatur\\files\\894\\Stroobant et al. - 2018 - Generating constrained length personalized bicycle.pdf:application/pdf},
}

@article{loepp_recommending_nodate,
	title = {Recommending Running Routes: Framework and Demonstrator},
	abstract = {Recommending personalized running routes is a challenging task. For considering the runner’s specific background as well as needs, preferences and goals, a recommender cannot only rely on a set of existing routes ran by others. Instead, each route must be generated individually, taking into account many different aspects that determine whether a suggestion will satisfy the runner in the end, e.g. height meters or areas passed. We describe a framework that summarizes these aspects and present a prototypical smartphone app that we implemented to actually demonstrate how personalized running routes can be recommended based on the different requirements a runner might have. A first preliminary study where users had to try this app and ran some of the recommended routes underlines the general effectiveness of our approach.},
	author = {Loepp, Benedikt and Ziegler, Jürgen},
	langid = {english},
	file = {Loepp und Ziegler - Recommending Running Routes Framework and Demonst.pdf:C\:\\Users\\Lisa Salewsky\\OneDrive\\Dokumente\\Uni\\Dortmund\\Master\\MA\\LS11 (Buchin)\\Literatur\\Ls11 Buchin MA Literatur\\files\\895\\Loepp und Ziegler - Recommending Running Routes Framework and Demonst.pdf:application/pdf},
}

@inproceedings{quercia_shortest_2014,
