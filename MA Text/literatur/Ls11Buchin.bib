@inproceedings{babaoglu_anthill_2002,
	location = {Vienna, Austria},
	title = {Anthill: a framework for the development of agent-based peer-to-peer systems},
	url = {https://ieeexplore.ieee.org/abstract/document/1022238?casa_token=jbaWnJiN4kUAAAAA:6apl0gGSFXWPi8DZoI9kfTY90nhFnuqL1j4uU-GohmkAlI3m9qmUAqn5PKxgOIxJ5k-QVnQd},
	doi = {10.1109/ICDCS.2002.1022238},
	shorttitle = {Anthill},
	abstract = {Recent peer-to-peer (P2P) systems are characterized by decentralized control, large scale and extreme dynamism of their operating environment. As such, they can be seen as instances of complex adaptive systems ({CAS}) typically found in biological and social sciences. We describe Anthill, a framework to support the design, implementation and evaluation of P2P applications based on ideas such as multi-agent and evolutionary programming borrowed from {CAS}. An Anthill system consists of a dynamic network of peer nodes; societies of adaptive agents travel through this network, interacting with nodes and cooperating with other agents in order to solve complex problems. Anthill can be used to construct different classes of P2P services that exhibit resilience, adaptation and self-organization properties. We also describe preliminary experiences with Anthill in implementing a file sharing application.},
	eventtitle = {Proceedings 22nd International Conference on Distributed Computing Systems},
	pages = {15--22},
	booktitle = {Proceedings 22nd International Conference on Distributed Computing Systems},
	publisher = {{IEEE}},
	author = {Babaoglu, O. and Meling, H. and Montresor, A.},
	urldate = {2023-10-22},
	date = {2002-07},
	note = {{ISSN}: 1063-6927},
	file = {IEEE Xplore Abstract Record:C\:\\Users\\Lisa Salewsky\\OneDrive\\Dokumente\\Uni\\Dortmund\\Master\\MA\\LS11 (Buchin)\\Literatur\\Ls11 Buchin MA Literatur\\files\\855\\1022238.html:text/html;IEEE Xplore Full Text PDF:C\:\\Users\\Lisa Salewsky\\OneDrive\\Dokumente\\Uni\\Dortmund\\Master\\MA\\LS11 (Buchin)\\Literatur\\Ls11 Buchin MA Literatur\\files\\853\\Babaoglu et al. - 2002 - Anthill a framework for the development of agent-.pdf:application/pdf},
